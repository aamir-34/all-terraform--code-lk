module "postgresql" {
  source                         = "../../../../../modules/cloudsql"
  name                           = var.psql_name
  region                         = var.region
  project                        = var.project
  network                        = var.network
  database_version               = var.psql_database_version
  activation_policy              = var.psql_activation_policy
  availability_type              = var.psql_availability_type
  retained_backups               = var.psql_retained_backups
  retention_unit                 = var.psql_retention_unit
  binary_log_enabled             = var.psql_binary_log_enabled
  enabled                        = var.psql_enabled
  point_in_time_recovery_enabled = var.psql_point_in_time_recovery_enabled
  start_time                     = var.psql_start_time
  transaction_log_retention_days = var.psql_transaction_log_retention_days
  disk_autoresize                = var.psql_disk_autoresize
  disk_autoresize_limit          = var.psql_disk_autoresize_limit
  disk_size                      = var.psql_disk_size
  disk_type                      = var.psql_disk_type
  ipv4_enabled                   = var.psql_ipv4_enabled
  private_network                = var.psql_private_network
  require_ssl                    = var.psql_require_ssl
  authorized_network_name        = var.psql_authorized_network_name
  authorized_network_value       = var.psql_authorized_network_value
  zone                           = var.zone
  day                            = var.psql_day
  hour                           = var.psql_hour
  pricing_plan                   = var.psql_pricing_plan
  tier                           = var.psql_tier
  user_labels                    = var.labels
  deletion_protection            = var.psql_deletion_protection
  kms_crypto_key_name            = var.kms_crypto_key_name
}
